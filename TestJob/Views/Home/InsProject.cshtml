@model InsProjectView



<h1>Create project</h1>


<div class="container">

    <form asp-action="InsProject" asp-controller="Home" method="post">

        <div>
            <input type="hidden"  asp-for="projectId" id="porjectId" />
        </div>

        <div class="form-group row">
            <div class="form-group row">
                <label asp-for="projectName" class="col-sm-3 col-form-label">ProjectName</label>
                <div class="col-sm-6">
                    <input type="text" asp-for="projectName" class="form-control"
                            id="projectName" />
                </div>

                <div class="w-100 mt-1  bg-dark"></div>
                <label asp-for="createDate" class="col-sm-3 col-form-label">CreateDate</label>
                <div class="col-sm-3">
                    <input type="text" asp-for="createDate" class="form-control" 
                            id="createDate" />
                </div>

                <div class="w-100 mt-1"></div>
                <label asp-for="createTime" class="col-sm-3 col-form-label">CreateTime</label>
                <div class="col-sm-3">
                    <input type="text" asp-for="createTime" class="form-control px-0" 
                            id="createTime" />
                </div>

                <div class="w-100 mt-1 bg-dark" style="height:2px"></div>
                <label asp-for="updateDate" class="col-sm-3 col-form-label">UpdateDate</label>
                <div class="col-sm-3">
                    <input type="text" asp-for="updateDate" class="form-control" 
                            id="updateDate" />
                </div>

                <div class="w-100 mt-1  bg-dark"></div>
                <label asp-for="updateTime" class="col-sm-3 col-form-label">UpdateTime</label>
                <div class="col-sm-3">
                    <input type="text" asp-for="updateTime" class="form-control px-0" 
                            id="updateTime" />  
                </div>
            </div>

            <div class="mt-2">
                    <button type="submit" class="btn btn-primary px-5 py-0">Save</button>
            </div>
        </div>

    </form>

</div>